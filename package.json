{
  "name": "axicreate",
  "version": "0.0.1",
  "description": "A creative coding tool for generating axidraw-compatible svgs.",
  "private": true,
  "main": "app/electron/main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "audit-app": "npx electronegativity -i ./ -x LimitNavigationGlobalCheck,PermissionRequestHandlerGlobalCheck",
    "translate": "node ./app/localization/translateMissing.js",
    "dev-server": "cross-env NODE_ENV=development webpack serve --config ./webpack.development.js > dev-scripts/webpack-dev-server.log 2> dev-scripts/webpack-dev-server-error.log",
    "dev": "concurrently --success last \"yarn node dev-scripts/prepareDevServer.js && yarn dev-server\" \"yarn node dev-scripts/launchDevServer.js\" -k",
    "prod-build": "cross-env NODE_ENV=production npx webpack --mode=production --config ./webpack.production.js",
    "prod": "yarn prod-build && electron .",
    "pack": "electron-builder --dir",
    "dist": "yarn test && yarn prod-build && electron-builder",
    "dist-mac": "yarn test && yarn prod-build && electron-builder --mac",
    "dist-linux": "yarn test && yarn prod-build && electron-builder --linux",
    "dist-windows": "yarn test && yarn prod-build && electron-builder --windows",
    "dist-all": "yarn test && yarn prod-build && electron-builder --mac --linux --windows",
    "test": "mocha"
  },
  "build": {
    "productName": "Axicreate",
    "appId": "com.electron.axicreate",
    "directories": {
      "buildResources": "resources"
    },
    "files": [
      "app/dist/**/*",
      "app/electron/**/*",
      "app/localization/!(locales)",
      "LICENSE"
    ],
    "extraFiles": [
      "app/localization/locales/**/*",
      "license.data",
      "public.key"
    ],
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ]
    }
  },
  "keywords": [
    "electron",
    "axidraw",
    "javascript",
    "react",
    "webpack",
    "typescript"
  ],
  "author": "JBWills",
  "license": "MIT",
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@doyensec/electronegativity": "^1.9.1",
    "@google-cloud/translate": "^6.2.2",
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/loadable__component": "^5.13.3",
    "@types/material-ui": "^0.21.8",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "csp-html-webpack-plugin": "^5.1.0",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "electron": "^13.1.2",
    "electron-builder": "^22.11.7",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "git+https://github.com/MarshallOfSound/electron-devtools-installer.git",
    "electron-mocha": "^10.1.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-autofix": "^1.0.5",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unused-imports": "^1.1.1",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.6.0",
    "mocha": "^8.4.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.3.1",
    "source-map-loader": "^3.0.0",
    "spectron": "^15.0.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "bulma": "^0.9.2",
    "history": "^4.7.2",
    "lodash": "4.17.21",
    "lodash.merge": "^4.6.2",
    "mini-create-react-context": "^0.4.1",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "scheduler": "^0.20.2",
    "secure-electron-context-menu": "^1.3.1",
    "secure-electron-license-keys": "^1.1.1",
    "secure-electron-store": "^3.0.1",
    "styled-components": "^5.1.10",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4",
    "use-context-selector": "^1.3.7"
  }
}
